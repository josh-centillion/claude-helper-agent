    "@modelcontextprotocol/sdk": "^1.25.2",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260129.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.61.1"
  }
